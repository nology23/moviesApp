      <ActionLangBody IsValid="True">
        <Script><![CDATA[if apiobjects.breadcrumbsCtrl.count() = 2 then
	apiobjects.breadcrumbsCtrl.addBreadcrumb("breadcrumbs", "Edit", "", "", false)
else
	apiobjects.breadcrumbsCtrl.addBreadcrumb("breadcrumbs", "Movie Detail", "", "", false)
	apiobjects.breadcrumbsCtrl.addBreadcrumb("breadcrumbs", "Edit", "", "", false)
endif

var movieData = apiobjects.serverConnection.getAsync("record?id=" + form.id)
if movieData.isError() then
	system.consoleError(movieData.getErrorMessage())
else
	form.complexEdit.fromJson(movieData.getJsonString())
	/*form.starring.fromJson(movieData.extractJson("starring"))
	form.genres.fromJson(movieData.extractJson("genres"))
	form.seasons1.fromJson(movieData.extractJson("seasons"))*/
endif

if form.complexEdit.type.enumValue() = "series" then
	form.editVisibility = true
else
	form.editVisibility = false
endif

system.console(movieData.getJsonString())]]></Script>
