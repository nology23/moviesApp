      <ActionLangBody IsValid="True">
        <Script><![CDATA[if form.complexEdit.type.enumValue() = "series" then
	form.editVisibility = true
else
	if form.complexEdit.seasons.hasValue() then
		form.confirmResult = apiobjects.page.confirm("This action will delete all seasons and episodes. Do you want that?")
		if form.confirmResult = true then
			form.complexEdit.seasons.clear()
			form.editVisibility = false
		else
			form.complexEdit.type.clear()
		endif		
	else
		form.editVisibility = false
	endif
endif]]></Script>
