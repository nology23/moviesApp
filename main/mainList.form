    <ActionBlock>
      <Name value="searchAction" />
      <ActionLangBody IsValid="True">
        <Script><![CDATA[var nameToLow = toLower(form.filter.list.name)
var starToLow = toLower(form.filter.starring.actorName)


// searching by type
if form.filter.type.hasValue() then
	if form.filter.type.enumValue() = "series" then
		form.actions.init()
		foreach item in form.movies do
			var movies = form.movies.selectFirst(current.type = "movie")
			movies.removeMe()
			form.deleteVisible = false
		endfor
	endif
	if form.filter.type.enumValue() = "movie" then
		form.actions.init()
			foreach item in form.movies do			
				var series = form.movies.selectFirst(current.type = "series")
				series.removeMe()	
				form.deleteVisible = false
		endfor
	endif
else
	form.actions.init()
	form.filter.name.setTouched(false)
	form.deleteVisible = false
endif

// searching by name
if form.filter.starring.hasValue() then
	form.movieSearch.clear()
	form.filter.starring.actorName = toLower(form.filter.starring.actorName)
	foreach item in form.movies do
		var actorNameLow = toLower(item.starring.toJson())
		if indexOf(actorNameLow, starToLow) <> 0 then
			var found = form.movieSearch.addItem()
			found.id = item.id
			found.name = item.name
			found.type = item.type
			found.yearOfRelease = item.yearOfRelease
			found.starring.fromJson(item.starring.toJson())
			found.rating.score = item.rating.score
		endif
	endfor
	form.moviesVisibility = false
	form.searchVisibility = true
endif	

// searching by name
if form.filter.list.hasValue() then
	form.movieSearch.clear()
	form.filter.list.name = toLower(form.filter.list.name)
	foreach item in form.movies do
		var nameLow = toLower(item.name)
		if indexOf(nameLow, nameToLow) <> 0 then
			var found = form.movieSearch.addItem()
			found.id = item.id
			found.name = item.name
			found.type = item.type
			found.yearOfRelease = item.yearOfRelease
			found.starring = item.starring
			found.rating.score = item.rating.score
			system.console(found.toJson())
		endif
	endfor
	form.moviesVisibility = false
	form.searchVisibility = true
endif	

if form.searchSort.enumValue() = "actorName" then
	form.actorNameVisible = true
else
	form.actorNameVisible = false
endif

if apiobjects.breadcrumbsCtrl.count() = 1 then
	apiobjects.breadcrumbsCtrl.addBreadcrumb("breadcrumbs", "Search", "", "", false)
else
	apiobjects.breadcrumbsCtrl.removeTo(1)
	apiobjects.breadcrumbsCtrl.addBreadcrumb("breadcrumbs", "Search", "", "", false)
endif]]></Script>
      </ActionLangBody>
      <DataContext>
        <ActionDataContext />
      </DataContext>
    </ActionBlock>
